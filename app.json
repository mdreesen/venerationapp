{
  "expo": {
    "name": "venerationapp",
    "slug": "venerationapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/v-logo-blackoutline.webp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/v-logo-blackoutline.webp",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/v-logo-blackoutline.webp",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/v-logo-blackoutline.webp"
    }
  }
}
